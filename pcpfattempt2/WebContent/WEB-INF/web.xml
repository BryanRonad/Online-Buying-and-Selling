<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>pcpfattempt2</display-name>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
    <servlet>
  	<servlet-name>LoginApp</servlet-name>
  	<servlet-class>pcpfattempt2.LoginApp</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>LoginApp</servlet-name>
  	<url-pattern>/LoginApp</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>RegisterUser</servlet-name>
  	<servlet-class>pcpfattempt2.RegisterUser</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>RegisterUser</servlet-name>
  	<url-pattern>/RegisterUser</url-pattern>
  </servlet-mapping>
  
    <servlet>
  	<servlet-name>AddListing</servlet-name>
  	<servlet-class>pcpfattempt2.AddListing</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>AddListing</servlet-name>
  	<url-pattern>/AddListing</url-pattern>
  </servlet-mapping>  
</web-app>